{
  "name": "github-openapi-types",
  "version": "1.0.0",
  "description": "Generated TypeScript definitions based on GitHub's OpenAPI spec",
  "main": "generated/types.ts",
  "repository": "https://github.com/gr2m/github-openapi-types",
  "keywords": [],
  "author": "Gregor Martynus (https://twitter.com/gr2m)",
  "license": "MIT",
  "scripts": {
    "download": "curl https://raw.githubusercontent.com/github/rest-api-description/main/descriptions/api.github.com/api.github.com.json --output cache/openapi-schema.json",
    "generate-types": "npx @manifoldco/swagger-to-ts cache/openapi-schema.json -o generated/types.ts"
  },
  "devDependencies": {
    "@manifoldco/swagger-to-ts": "^2.1.0"
  }
}
